<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomEditTextStyleable">
        <attr name="android:text" />
        <attr name="android:hint" />
        <attr name="android:required" />
        <attr name="android:maxLength" />
        <attr name="customEditTextType">
            <flag name="text" value="0" />
            <flag name="number" value="1" />
            <flag name="money" value="2" />
            <flag name="phone" value="3" />
            <flag name="cpf" value="4" />
            <flag name="cnpj" value="5" />
        </attr>
    </declare-styleable>

</resources>