<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomButtonStyleable">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:enabled" />
        <attr name="android:textSize" />
        <attr name="android:fontFamily" />
        <attr name="customButtonIcon" format="reference"/>
        <attr name="customButtonIsLoading" format="boolean"/>
        <attr name="customButtonDisabledTextColor" format="color" />
        <attr name="customButtonType">
            <flag name="default" value="0" />
            <flag name="whatsapp" value="1" />
            <flag name="phone" value="2" />
        </attr>
    </declare-styleable>

</resources>